<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>BServlet</servlet-name>
        <servlet-class>cc.sky.servlet.BaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BServlet</servlet-name>
        <url-pattern>/BServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CServlet</servlet-name>
        <servlet-class>cc.sky.servlet.CServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CServlet</servlet-name>
        <url-pattern>/CServlet</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>AFilter</filter-name>
        <filter-class>cc.sky.servlet.AFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AFilter</filter-name>
        <url-pattern>/u</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>BFilter</filter-name>
        <filter-class>cc.sky.servlet.BFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>BFilter</filter-name>
        <url-pattern>/*</url-pattern>

    </filter-mapping>
    <filter>
        <filter-name>mFilter</filter-name>
        <filter-class>cc.sky.servlet.MyResponseFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>mFilter</filter-name>
        <url-pattern>/dServlet</url-pattern>
        <!--<url-pattern>/MyServlet</url-pattern>-->
        <url-pattern>/l</url-pattern>
    </filter-mapping>
    <filter>
        <filter-name>GFilter</filter-name>
        <filter-class>cc.sky.servlet.GZIPFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>GFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>mServlet</servlet-name>
        <servlet-class>cc.sky.servlet.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MServlet</servlet-name>
        <servlet-class>cc.practise.MServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MServlet</servlet-name>
        <url-pattern>/MServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>cc.sky.servlet.IpListener</listener-class>
    </listener>
    <session-config>
        <session-timeout>1</session-timeout>
    </session-config>
</web-app>